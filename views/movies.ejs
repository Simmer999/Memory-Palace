<%- include('./partials/head'); %>
  <section>
    <%- include('./partials/nav'); %>
      <div class='title' style='width: 60%'>
          <ul class="quotes">
              <!-- Loop through quotes -->
              <% for(var i = 0; i < entries.length; i++) {%>
                <li class="quote">
                  <!-- These are entries from the 'movies' collection> -->
                  <span><%= entries[i].title%></span>
                  <br>
                  : <span><%= entries[i].year%></span>
                  <br>
                : <span><%= entries[i].plot%></span>
                </li>
                <br>
                <!-- End -->
              <% } %>
            </ul>
      </div>
  </section>
<%- include('./partials/footer'); %>  